{
  "name": "@julienbreux/clamp",
  "version": "1.0.4",
  "description": "Clamp is a useful tool to help to replace environment variables in any file using go template syntax.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JulienBreux/clamp.git"
  },
  "keywords": [
    "env",
    "vars"
  ],
  "author": {
    "name": "Julien Breux",
    "email": "julien.breux@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JulienBreux/clamp/issues"
  },
  "homepage": "https://github.com/JulienBreux/clamp#readme",
  "dependencies": {
    "go-npm": "0.1.9"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "postinstall": "go-npm install",
    "preuninstall": "go-npm uninstall"
  },
  "goBinary": {
    "name": "clamp",
    "path": "./bin",
    "url": "https://github.com/JulienBreux/clamp/releases/download/v{{version}}/clamp_{{version}}_{{platform}}_{{arch}}.tar.gz"
  },
  "files": []
}
